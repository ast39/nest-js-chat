{
  "name": "prodavato-chat-micro-service",
  "version": "1.0.0",
  "description": "Microservice for meeting between users",
  "author": "ASt",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "app": "yarn prisma generate && yarn prisma migrate deploy && if [ \"$NODE_ENV\" = \"production\" ]; then node dist/src/main; else nest start --watch; fi",
    "start:dev": "yarn prisma generate && yarn prisma migrate deploy && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn prisma generate && yarn prisma migrate deploy && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:version": "yarn prisma version",
    "prisma:debug": "yarn prisma debug",
    "prisma:format": "yarn prisma format",
    "prisma:validate": "yarn prisma validate",
    "prisma:generate": "yarn prisma generate",
    "prisma:studio": "yarn prisma studio",
    "prisma:pull": "yarn prisma db pull",
    "prisma:push": "yarn prisma db push",
    "prisma:seed": "yarn prisma db seed",
    "migrate:create": "yarn prisma migrate dev --create-only",
    "migrate:deploy": "yarn prisma migrate deploy",
    "migrate:reset": "yarn prisma migrate reset",
    "migrate:resolve": "yarn prisma migrate resolve"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.606.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.3.7",
    "@prisma/client": "^5.15.1",
    "add": "^2.0.6",
    "aws-sdk": "^2.1651.0",
    "aws-sdk-v3-nest": "^0.3.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "fluent-ffmpeg": "^2.1.3",
    "heic-convert": "^2.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "uuid": "^10.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/heic-convert": "^1.2.3",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
